%!PS
/Courier
40 selectfont 

/s 2 string def

realtime
srand

/printnum {
rand realtime add % Some super-hacky randomness based on execution time
srand
rand
99 mod
1 add
} def

/GenNums {
numDict begin
rand
99 mod
1 add
end
} def

/boxWidth 60 def
/boxHeight 50 def

/IncX {/x x boxWidth add def} def
/ResX {/x baseX def} def
/DecY {/y y boxHeight sub def} def
/ResY {/y baseY def} def
/IncCount {/counter counter 1 add def} def

/baseX 72 def
/baseY 500 def
ResX
ResY
/counter 0 def

/numDict 24 dict def
numDict begin

numDict length
72 140 moveto
s cvs show
end

25 {
counter 12 eq
{
( *) show
}
{
x y moveto
printnum
dup
10 lt {
x 11 add
y moveto
} if
s cvs show
} ifelse

IncX
IncCount

counter 5 mod
0 eq {ResX DecY} if

} repeat

%
% Draw grid
%
/baseX 65 def
/endX 365 def
/baseY 540 def
/endY 290 def
ResX
ResY

/HLine {
newpath
baseX y moveto
endX y lineto
stroke
} def

/VLine {
newpath
x baseY moveto
x endY lineto
stroke
} def

6 {
HLine
VLine
IncX
DecY
} repeat

showpage
